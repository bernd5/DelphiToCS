  vcl,
  vcldb,
  dbrtl,
  vclx,
  Common,
  VclSmp;

contains
  Wizard in 'Wizard.pas' {frmWizard},
  ClipboardData in 'ClipboardData.pas',
  ControlUtils in 'ControlUtils.pas',
  BitmapMessageBox in 'BitmapMessageBox.pas' {frmBitmapMessageBox},
  StringLoader in 'StringLoader.pas',
  GetNames in 'GetNames.pas' {GetNameDlg},
  DialogUtils in 'DialogUtils.pas',
  InputDialog in 'InputDialog.pas' {frmInputDialog};

end.
